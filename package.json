    "drizzle-orm": "^0.30.10",
    "graphql": "^16.8.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.7",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "drizzle-kit": "^0.21.1"
  },
  "keywords": [
    "drizzle",
    "pg",
    "mysql",
    "postgresql",
    "postgres",
    "sqlite",
    "database",
    "sql",
    "typescript",
    "ts",
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drizzle-team/drizzle-graphql.git"
  },
