{
  "name": "drizzle-schema-generator",
  "type": "module",
  "author": "Edsol",
  "version": "0.0.1",
  "description": "Automatically generate of Drizzle schema from existing database",
  "main": "index.js",
  "scripts": {
    "start": "bun src/test.ts",
    "dev": "bun --watch src/test.ts",
    "build": "bun build --compile ./src/main.ts --outfile=dist/app",
    "prod": "bun run dist/app",
    "test": "no test"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "dotenv": "^16.4.5",
